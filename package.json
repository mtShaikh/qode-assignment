{
  "name": "qode-assigmnet",
  "version": "0.1.0",
  "private": true,
  "author": "mtShaikh",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "turbo run build",
    "start": "next start",
    "lint": "next lint",
    "prebuild": "prisma generate && prisma migrate deploy",
    "postinstall": "prisma generate",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "turbo run lint",
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "turbo run type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@prisma/client": "^5.1.1",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.21",
    "js-cookie": "^3.0.5",
    "next": "^13.4.10",
    "prisma": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.2.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.x.x",
    "pnpm": ">=8"
  }
}
